{
    "manifest_version": 3,
    "name": "Smart Calendar",
    "version": "1.0",
    "description": "Calendar that provides various functionalities to the user",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png",
        "default_title": "Smart Calendar"
    }, 
    "permissions": ["activeTab"],

    "background": {
        "server_worker": "background.js"
    }
}